bring xcs.class34;

func main() {
    let x = 10;
    let y = 20;
    let sum = x + y;
    say "Sum: " + sum;
    
    const pi = 3.14;
    say "Pi: " + pi;
    
    if x < y {
        say "x is less than y";
    } else {
        say "x is greater than or equal to y";
    }
    
    loop i in 1 to 5 {
        say "Loop iteration: " + i;
    }
}
