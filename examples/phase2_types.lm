bring xcs.class34;

func greet(name) {
    say "Hey " + name + "!";
}

func sum(a:int, b:int):int {
    return a + b;
}

type Car {
    val brand;
    val speed;

    func run() {
        say brand + " is running at " + speed;
    }
}

func main() {
    greet("Sir");
    
    let result = sum(10, 20);
    say "Sum result: " + result;
    
    val car = Car("BMW", 220);
    car.run();
}
